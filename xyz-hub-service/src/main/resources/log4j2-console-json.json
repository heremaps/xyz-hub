{
  "configuration": {
    "status": "info",

    "name": "Default Log Config",
    "packages": "com.here.xyz",

    "ThresholdFilter": {
      "level": "info"
    },
    "CustomLevels": {
      "CustomLevel": [{
        "name": "STREAM",
        "intLevel": 350
      }]
    },
    "appenders": {
      "Console": {
        "name": "STDOUT",
        "PatternLayout": {
          "MarkerPatternSelector": {
            "defaultPattern": "{\"t\":\"%p\",\"time\":\"%d{ISO8601}\",\"unixtime\":\"%d{UNIX_MILLIS}\",\"msg\":\"%replace{%replace{%.-4096msg%throwable}{[\n]}{\\\\n}}{[\\\"]}{\\\\\"}\",\"streamId\":\"%marker\"}%n%xEx{none}",
            "PatternMatch": {
              "key": "ACCESS",
              "pattern": "%m%n"
            }
          }
        }
      }
    },
    "loggers": {
      "root": {
        "level": "info",
        "AppenderRef": {
          "ref": "STDOUT"
        }
      }
    }
  }
}
