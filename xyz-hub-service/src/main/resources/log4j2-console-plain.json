{
  "configuration": {
    "status": "info",
    "name": "Default Log Config",
    "packages": "com.here.xyz"
  },
  "appenders": {
    "Console": {
      "name": "STDOUT",
      "PatternLayout": {
        "pattern": "%d %-5p %c{1} %.-4096msg %enc{%ex}{JSON}%n%xEx{none}"
      }
    }
  },
  "loggers": {
    "root": {
      "level": "info",
      "AppenderRef": {
        "ref": "STDOUT"
      }
    }
  }
}
